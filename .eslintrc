{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "es6": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "react/prop-types": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "react/forbid-prop-types": 0,
    "jsx-a11y/anchor-has-content": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "no-unused-vars": 0

  },
  "env": {
    "browser": true,
    "es6": true
  }
}
